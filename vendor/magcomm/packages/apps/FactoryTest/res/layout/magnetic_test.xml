<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_margin="@dimen/dp_20"
    android:background="@color/white"
    android:orientation="vertical"
    android:paddingTop="@dimen/px_100">

    <TextView
        android:id="@+id/tv_magnetic_title"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:textColor="@color/black"
        android:textSize="@dimen/sp_24" />

    <TextView
        android:id="@+id/tv_magnetic_datax"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/dp_8"
        android:textColor="@color/black"
        android:textSize="@dimen/sp_24" />

    <TextView
        android:id="@+id/tv_magnetic_datay"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/dp_8"
        android:textColor="@color/black"
        android:textSize="@dimen/sp_24" />

    <TextView
        android:id="@+id/tv_magnetic_dataz"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/dp_8"
        android:textColor="@color/black"
        android:textSize="@dimen/sp_24" />

    <TextView
        android:id="@+id/tv_magnetic_quality"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/dp_8"
        android:textColor="@color/black"
        android:textSize="@dimen/sp_24"
        android:visibility="gone" />

    <TextView
        android:id="@+id/tv_magnetic_notice"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/dp_20"
        android:gravity="center"
        android:textColor="@color/black"
        android:textSize="@dimen/sp_24" />

</LinearLayout>